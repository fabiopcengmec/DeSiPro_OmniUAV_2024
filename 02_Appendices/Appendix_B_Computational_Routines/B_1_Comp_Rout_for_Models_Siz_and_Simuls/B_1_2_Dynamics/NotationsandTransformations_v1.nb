Cell[BoxData[
 RowBox[{
  RowBox[{"EVectorOrTMatrix", "[", 
   RowBox[{"entity_", ",", "option_"}], 
   RowBox[{"(*", 
    RowBox[{"EV", " ", "=", " ", 
     RowBox[{
      RowBox[{"Euler", " ", "or", " ", "TM"}], " ", "=", " ", 
      RowBox[{"Transformation", " ", "Matrix"}]}]}], "*)"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "test", ",", "fnew", ",", " ", "Tanti", ",", " ", "Tsimet", ",", 
      "twister", ",", "twisterVersor", ",", " ", "theta", ",", "sintheta", 
      ",", "costheta", ",", " ", "twisterTil", ",", " ", "EV", ",", " ", 
      "TM"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{"Kernel", " ", "Function", " ", "of", " ", "solution"}], " ", 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"test", "[", 
       RowBox[{"fnc_", ",", "cmd_"}], "]"}], ":=", 
      RowBox[{"fnc", "==", "cmd"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Loop", " ", "of", " ", "solution"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"test", "[", 
           RowBox[{"option", ",", "EV"}], "]"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Tanti", "=", 
            RowBox[{
             FractionBox["1", "2"], " ", 
             RowBox[{"(", 
              RowBox[{"entity", "-", 
               RowBox[{"Transpose", "[", "entity", "]"}]}], ")"}]}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Tsimet", "=", 
            RowBox[{
             FractionBox["1", "2"], " ", 
             RowBox[{"(", 
              RowBox[{"entity", "+", 
               RowBox[{"Transpose", "[", "entity", "]"}]}], ")"}]}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"twister", "=", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"Tanti", "[", 
               RowBox[{"[", 
                RowBox[{"3", ",", "2"}], "]"}], "]"}], ",", 
              RowBox[{"Tanti", "[", 
               RowBox[{"[", 
                RowBox[{"1", ",", "3"}], "]"}], "]"}], ",", 
              RowBox[{"Tanti", "[", 
               RowBox[{"[", 
                RowBox[{"2", ",", "1"}], "]"}], "]"}]}], "}"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"twisterVersor", "=", 
            FractionBox["twister", 
             RowBox[{"Norm", "[", "twister", "]"}]]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"sintheta", "=", 
            RowBox[{"Norm", "[", "twister", "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"theta", "=", 
            RowBox[{"ArcSin", "[", "sintheta", "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"fnew", "=", 
            RowBox[{"{", 
             RowBox[{"twisterVersor", ",", "twister", ",", "theta"}], 
             "}"}]}]}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"test", "[", 
           RowBox[{"option", ",", "TM"}], "]"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"twister", "=", 
            RowBox[{"entity", "[", 
             RowBox[{"[", 
              RowBox[{"1", ";;", "3"}], "]"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"theta", "=", 
            RowBox[{"entity", "[", 
             RowBox[{"[", "4", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"sintheta", "=", 
            RowBox[{"Sin", "[", "theta", "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"costheta", "=", 
            RowBox[{"Cos", "[", "theta", "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"twisterTil", "=", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"0", ",", 
                RowBox[{"-", 
                 RowBox[{
                  RowBox[{"Evaluate", "[", 
                   FractionBox["twister", 
                    RowBox[{"Norm", "[", "twister", "]"}]], "]"}], "[", 
                  RowBox[{"[", "3", "]"}], "]"}]}], ",", 
                RowBox[{
                 RowBox[{"Evaluate", "[", 
                  FractionBox["twister", 
                   RowBox[{"Norm", "[", "twister", "]"}]], "]"}], "[", 
                 RowBox[{"[", "2", "]"}], "]"}]}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{
                 RowBox[{"Evaluate", "[", 
                  FractionBox["twister", 
                   RowBox[{"Norm", "[", "twister", "]"}]], "]"}], "[", 
                 RowBox[{"[", "3", "]"}], "]"}], ",", "0", ",", 
                RowBox[{"-", 
                 RowBox[{
                  RowBox[{"Evaluate", "[", 
                   FractionBox["twister", 
                    RowBox[{"Norm", "[", "twister", "]"}]], "]"}], "[", 
                  RowBox[{"[", "1", "]"}], "]"}]}]}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"-", 
                 RowBox[{
                  RowBox[{"Evaluate", "[", 
                   FractionBox["twister", 
                    RowBox[{"Norm", "[", "twister", "]"}]], "]"}], "[", 
                  RowBox[{"[", "2", "]"}], "]"}]}], ",", 
                RowBox[{
                 RowBox[{"Evaluate", "[", 
                  FractionBox["twister", 
                   RowBox[{"Norm", "[", "twister", "]"}]], "]"}], "[", 
                 RowBox[{"[", "1", "]"}], "]"}], ",", "0"}], "}"}]}], "}"}]}],
            ";", "\[IndentingNewLine]", 
           RowBox[{"fnew", "=", 
            RowBox[{
             RowBox[{"IdentityMatrix", "[", "3", "]"}], "+", 
             RowBox[{"sintheta", " ", "twisterTil"}], "+", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"1", "-", 
                 RowBox[{"Cos", "[", "theta", "]"}]}], ")"}], " ", 
               RowBox[{"MatrixPower", "[", 
                RowBox[{"twisterTil", ",", "2"}], "]"}]}], ")"}]}]}]}]}], 
         "]"}]}], "\[IndentingNewLine]", ",", "1"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "fnew", "]"}]}]}], " ", 
